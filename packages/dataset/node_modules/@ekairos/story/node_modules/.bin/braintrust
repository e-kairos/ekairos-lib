#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/braintrust@0.3.7_@aws-sdk+c_9fb9e959798970c92197c1815e6a0c67/node_modules/braintrust/dist/node_modules:/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/braintrust@0.3.7_@aws-sdk+c_9fb9e959798970c92197c1815e6a0c67/node_modules/braintrust/node_modules:/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/braintrust@0.3.7_@aws-sdk+c_9fb9e959798970c92197c1815e6a0c67/node_modules:/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/braintrust@0.3.7_@aws-sdk+c_9fb9e959798970c92197c1815e6a0c67/node_modules/braintrust/dist/node_modules:/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/braintrust@0.3.7_@aws-sdk+c_9fb9e959798970c92197c1815e6a0c67/node_modules/braintrust/node_modules:/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/braintrust@0.3.7_@aws-sdk+c_9fb9e959798970c92197c1815e6a0c67/node_modules:/mnt/c/Users/aleja/storias/projects/pulzar/pulzar-lib-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../braintrust/dist/cli.js" "$@"
else
  exec node  "$basedir/../braintrust/dist/cli.js" "$@"
fi
